#FortiProxy transprant policy initial provision
<INIT>
[LOCAL]

[GLOBAL]

<SETUP>
[FPX1]
conf fire policy
    edit ($DEFAULT_PROXY_ID)
	set status disable
    next
    edit ($DEFAULT_PROXY_IBP)
	set status enable
    next
end

<TEARDOWN>
[FPX1]
conf fire policy
    edit ($DEFAULT_PROXY_ID)
	set status enable
    next
    edit ($DEFAULT_PROXY_IBP)
	set status disable
    next
end
